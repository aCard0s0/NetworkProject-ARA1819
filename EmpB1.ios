! Entre Aveiro e porta f1/0 ainda não á comunicação
conf t

ip name-server 10.5.100.100
ip domain lookup

! Aveiro connection
interface FastEthernet0/0
    ip address 10.2.30.2 255.255.0.0
    ip ospf 1 area 0
    no shutdown

! NetB1 connection
interface FastEthernet1/0
    ip address 111.1.1.1 255.255.255.0
    ip ospf 1 area 0
    no shutdown

end